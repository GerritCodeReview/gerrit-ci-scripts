[access]
	inheritFrom = Public-Projects
[project]
	description = Scripts used for continuous integration builds of Gerrit
[access "refs/*"]
	owner = group gerrit-ci-scripts-maintainers
	owner = group google/gerritcodereview-maintainers@googlegroups.com
	read = group gerrit-ci
[submit]
	action = rebase if necessary
[label "Code-Style"]
	function = NoBlock
	value = -1 Wrong Style or Formatting
	value = 0 No score
	value = +1 Style Verified
	defaultValue = 0
	copyCondition = changekind:NO_CHANGE OR changekind:NO_CODE_CHANGE
[access "refs/heads/master"]
	create = group gerrit-ci-scripts-maintainers
	label-Code-Review = -2..+2 group gerrit-ci-scripts-maintainers
	label-Code-Style = -1..+1 group gerrit-verifiers
	label-Verified = -1..+1 group Change Owner
	label-Verified = -1..+1 group gerrit-ci-scripts-maintainers
	submit = group gerrit-ci-scripts-maintainers
[access "refs/heads/private"]
	read = group gerrit-ci
	read = group gerrit-ci-scripts-maintainers
	exclusiveGroupPermissions = read
	label-Code-Style = -1..+1 group gerrit-ci
	label-Code-Review = -2..+2 group gerrit-ci-scripts-maintainers
[submit-requirement "Code-Style"]
	description = Submit requirement for the 'Code-Style' label
	submittableIf = label:Code-Style=MAX AND -label:Code-Style=MIN
	canOverrideInChildProjects = true
